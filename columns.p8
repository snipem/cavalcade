pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
-- hello world
-- by zep
-- run: ./reload.sh

board = {}          -- create the matrix
bottom_left = 32
bottom_right = 96

function reset_board()
    printh("Resetting board...")
    for i=0,16 do
    board[i] = {}     -- create a new row
        for j=0,5 do
            slot = {}
            slot.empty = false
            -- start from below
            spacer = 8
            slot.x = spacer + bottom_left + j * 8
            slot.y = i * 8
            board[i][j] = slot
        end
    end

end

function _init()
    reset_board()
end

function _update ()
    -- Draw backgorund
    cls()
    rectfill(0, 0, 128, 128, 7)
    rectfill(bottom_left, 0, bottom_right, 128, 0)

    for i=0,16 do
        for j=0,5 do
            printh(board[i][j].x)
            spr(1, board[i][j].x, board[i][j].y)
            -- slot = {}
            -- slot.empty = false
            -- slot.x = i * 8
            -- slot.y = j * 8
            -- board[i][j] = slot
        end
    end

    flip()
end


--- hack for using external editor
if peek(0x4300) == 0 then
    poke(0x4300,1)
    printh("------- Reloading")
    load("columns")
    run()
else
    poke(0x4300,0)
end
